# spouse
:P26_SymmetricShape 
	a sh:NodeShape ;
	sh:targetSubjectsOf wdt:P26;
	sh:property [ 
      sh:path [sh:inversePath wdt:P26] ;
      sh:equals wdt:P26;
      sh:minCount 1 ;
    ] .